<?xml version="1.0"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
        http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
   <application>
      <resource-bundle>
         <base-name>com.cts.messages</base-name>
         <var>msgs</var>
      </resource-bundle>
   </application>

   <managed-bean>
      <managed-bean-name>tableData</managed-bean-name>
      <managed-bean-class>com.cts.TableData</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
   </managed-bean>
   <managed-bean>
      <managed-bean-name>tableData1</managed-bean-name>
      <managed-bean-class>com.cts.TableData1</managed-bean-class>
      <managed-bean-scope>session</managed-bean-scope>
   </managed-bean>
</faces-config>
